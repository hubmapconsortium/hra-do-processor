ccf:FtuIllustration a owl:Class ;
    rdfs:label "FtuIllustration" ;
    ccf:owl "NamedIndividual" ;
    rdfs:subClassOf ccf:Instance,
        ccf:Named ;
    skos:exactMatch ccf:FtuIllustration ;
