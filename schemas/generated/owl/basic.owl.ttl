ccf:orcid a owl:DatatypeProperty ;
    rdfs:label "orcid" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:onDatatype xsd:string ;
            owl:withRestrictions ( [ xsd:pattern "^(\\d{4}-){3}\\d{3}(\\d|X)$" ] ) ] ;
    skos:inScheme <https://purl.humanatlas.io/specs/basic> .

