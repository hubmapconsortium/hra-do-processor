<https://purl.humanatlas.io/specs/collection-metadata.owl.ttl> a owl:Ontology ;
    rdfs:label "collection-metadata" ;
    IAO:0000700 dcat:Container,
        dcat:DatasetMetadata,
        dcat:Distribution,
        dcat:Grant,
        dcat:Person ;
    linkml:id "https://purl.humanatlas.io/specs/collection-metadata" .

linkml:SubsetDefinition a owl:Class .

schema1:about a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "about" ;
    rdfs:range dcat:String ;
    skos:exactMatch schema1:about .

schema1:propertyID a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "propertyId" ;
    rdfs:range dcat:String ;
    skos:exactMatch schema1:propertyID .

schema1:value a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "value" ;
    rdfs:range dcat:String ;
    skos:exactMatch schema1:value .

owl:versionInfo a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "versionInfo" ;
    rdfs:range dcat:String ;
    skos:exactMatch owl:versionInfo .

dcat:Container a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Container" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty dct:publisher ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom dcat:Distribution ;
                        owl:onProperty dcat:distribution ] [ a owl:Restriction ;
                        owl:onProperty dcat:distribution ;
                        owl:someValuesFrom dcat:Distribution ] ) ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty ccf:citation_overall ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty schema1:citation ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat:Person ;
            owl:onProperty ccf:project_lead ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat:Person ;
            owl:onProperty ccf:external_reviewer ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:Uriorcurie ;
            owl:onProperty ccf:doi ],
        [ a owl:Restriction ;
            owl:onClass dcat:String ;
            owl:onProperty dct:description ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass dcat:String ;
            owl:onProperty schema1:version ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass dcat:String ;
            owl:onProperty dct:license ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass dcat:String ;
            owl:onProperty dct:title ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat:Grant ;
            owl:onProperty schema1:funding ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom dcat:Person ;
                        owl:onProperty dct:creator ] [ a owl:Restriction ;
                        owl:onProperty dct:creator ;
                        owl:someValuesFrom dcat:Person ] ) ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat:Person ;
            owl:onProperty schema1:reviewedBy ],
        [ a owl:Restriction ;
            owl:onClass dcat:String ;
            owl:onProperty dcat:iri ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty ccf:hubmap_id ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat:String ;
            owl:onProperty ccf:data_table ],
        [ a owl:Restriction ;
            owl:onClass dcat:String ;
            owl:onProperty schema1:dateCreated ;
            owl:qualifiedCardinality 1 ] ;
    skos:exactMatch dcat:Dataset .

dcat:DatasetMetadata a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DatasetMetadata" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty schema1:dateCreated ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty schema1:version ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty dct:license ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:Uriorcurie ;
            owl:onProperty rdfs:seeAlso ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty dct:description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty dct:publisher ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcat:String ;
            owl:onProperty dct:title ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcat:Person ;
            owl:onProperty dct:creator ] ;
    skos:exactMatch dcat:Dataset .

