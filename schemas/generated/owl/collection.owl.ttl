<https://purl.humanatlas.io/specs/collection.owl.ttl> a owl:Ontology ;
    rdfs:label "collection" ;
    IAO:0000700 ccf:CollectionMetadata,
        ccf:Container,
        ccf:DatasetMetadata,
        ccf:Distribution,
        ccf:Grant,
        ccf:Person ;
    linkml:id "https://purl.humanatlas.io/specs/collection" .

linkml:SubsetDefinition a owl:Class .

ccf:Container a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Container" ;
    ccf:owl.template """AnnotationAssertion( dct:title {{iri}} "{{metadata.title|e}}" )
AnnotationAssertion( dct:description {{iri}} "{{metadata.description|e}}" )
{% for c in metadata.creators %}
AnnotationAssertion( dct:creator {{iri}} "{{c.fullName|e}} ({{c.orcid}})" )
{% endfor %}
AnnotationAssertion( schema:version {{iri}} "{{metadata.version|e}}" )
AnnotationAssertion( schema:dateCreated {{iri}} "{{metadata.creation_date|e}}" )
AnnotationAssertion( dct:license {{iri}} "{{metadata.license|e}}" )
AnnotationAssertion( dct:publisher {{iri}} "{{metadata.publisher|e}}" )
AnnotationAssertion( rdfs:seeAlso {{iri}} "{{metadata.see_also|e}}" )""" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:Uriorcurie ;
            owl:onProperty ccf:iri ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom ccf:String ;
                        owl:onProperty ccf:data ] [ a owl:Restriction ;
                        owl:onProperty ccf:data ;
                        owl:someValuesFrom ccf:String ] ) ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:CollectionMetadata ;
            owl:onProperty ccf:metadata ] .

ccf:DatasetMetadata a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "DatasetMetadata" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:String ;
            owl:onProperty schema1:dateCreated ],
        [ a owl:Restriction ;
            owl:allValuesFrom ccf:Person ;
            owl:onProperty dct:creator ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:Uriorcurie ;
            owl:onProperty rdfs:seeAlso ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:String ;
            owl:onProperty dct:title ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:String ;
            owl:onProperty dct:description ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:String ;
            owl:onProperty schema1:version ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:String ;
            owl:onProperty dct:license ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass ccf:String ;
            owl:onProperty dct:publisher ] ;
    skos:exactMatch dcat:Dataset .

